{
  "manifest_version": 2,
  "name": "Web Watcher",
  "version": "1.0.0", "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Content.js"]
    }
  ],
  "icons": { "48": "images/icon48.png", "128": "images/icon128.png" },
  "browser_action": {
    "default_popup": "Popup.html",
    "default_title": "Web Watcher"
  },
  "permissions": [
    "https://www.youtube.com/*",
    "https://www.facebook.com/*",
    "https://www.netflix.com/*"
  ]
}
